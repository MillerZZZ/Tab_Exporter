{
  "manifest_version": 3,
  "name": "Tab Exporter Pro",
  "version": "3.0",
  "permissions": [
    "tabs", 
    "downloads", 
    "tabGroups", 
    "contextMenus", 
    "storage", 
    "scripting"
  ],
  "host_permissions": [
    "*://gemini.google.com/*"
  ],
  "action": {
    "default_title": "导出标签页存档"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}